<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- 指定id和name，id就是标识符，而name就是别名，标识符必须在IOC容器中唯一 -->
    <bean id="declarebean4_id1" name="declarebean4_alias1" class="com.mynawang.test.HelloApiImpl"></bean>

    <!-- 如果id和name一样，IoC容器能检测到，并消除冲突 -->
    <bean id="declarebean4_2" name="declarebean4_2" class="com.mynawang.test.HelloApiImpl"></bean>

</beans>